{
  "name": "gatsby-contentful-starter",
  "description": "Starter Contentful Gatsby Blog",
  "version": " ^1.0.0",
  "author": "Stefan Judis <stefanjudis@gmail.com>",
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ],
  "bugs": {
    "url": "https://github.com/contentful-userland/gatsby-contentful-starter/issues"
  },
  "dependencies": {
    "@netlify/plugin-gatsby": "^3.5.1",
    "algoliasearch": "^3.35.0",
    "babel-plugin-styled-components": "^2.0.7",
    "chalk": "^4.1.2",
    "corejs": "^1.0.0",
    "disqus-react": "^1.1.5",
    "gatsby-cli": "^5.4.0",
    "gatsby-plugin-algolia": "^1.0.3",
    "gatsby-plugin-feed": "^5.4.0",
    "gatsby-plugin-image": "^3.5.0",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-preload-fonts": "^4.5.0",
    "gatsby-plugin-react-helmet": "^6.5.0",
    "gatsby-plugin-sharp": "^5.5.0",
    "gatsby-plugin-styled-components": "^6.5.0",
    "gatsby-remark-responsive-iframe": "^6.5.0",
    "gatsby-source-contentful": "^8.5.0",
    "gatsby-source-filesystem": "^5.4.0",
    "gatsby-transformer-remark": "^6.4.0",
    "gatsby-transformer-sharp": "^5.4.0",
    "gbimage-bridge": "^0.2.2",
    "local-storage-fallback": "^4.1.2",
    "lodash": "^4.17.11",
    "react": "^18.0.0 || ^18.2.0",
    "react-autosuggest": "^9.4.3",
    "react-awesome-slider": "^4.1.0",
    "react-dom": "^18.2.0",
    "react-headroom": "^3.2.1",
    "react-helmet": "^6.1.0",
    "react-instantsearch-dom": "^6.38.3",
    "react-social-media-icons": "^10.0.4",
    "react-social-sharing": "^3.3.0",
    "react-spring": "^8.0.27",
    "sharp": "^0.31.3 || ^0.30.1",
    "styled-components": "^5.3.6",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "contentful-import": "^8.5.22",
    "core-js": "^2.6.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.32.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "^5.4.2",
    "gatsby-plugin-netlify": "^5.1.0",
    "gh-pages": "^2.0.1",
    "inquirer": "^6.2.2",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "yargs-parser": "^13.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.32.0 || ^7.0.0",
    "react": "^18.0.0 || ^18.2.0",
    "sharp": "^0.31.3 || ^0.30.1"
  },
  "homepage": "https://github.com/contentful-userland/gatsby-contentful-starter#readme",
  "keywords": [
    "gatsby",
    "contentful"
  ],
  "license": "MIT",
  "main": "/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful-userland/gatsby-contentful-starter.git"
  },
  "scripts": {
    "preload-fonts": "gatsby-preload-fonts",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "postinstall": "node ./bin/hello.js",
    "setup": "node ./bin/setup.js",
    "start": "npm run develop",
    "heroku-postbuild": "gatsby build",
    "cleanup-repository": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
  }
}
